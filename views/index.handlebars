<div class="carousel carousel-slider center">
    {{#each stores}}
    <div class="carousel-item  white-text" style="height: 100px" href="#{{this.id}}!">
      <img src="{{this.storeImg}}">
    </div>
    {{/each}}
  </div>
<div class="container">
<div class="row">
  {{#each service}}
    <div class="col s12 m3">
      <div class="card">
        <div class="card-image">
          <img src="{{this.serviceImg}}">
          <a class="btn-floating halfway-fab waves-effect waves-light red" href="/service/{{this.id}}"><i class="material-icons">zoom_in</i></a>
        </div>
        <div class="card-content">
          <span class="card-title">{{this.serviceTitle}}</span>
          <p>{{this.serviceDesc}}</p>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
  </div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

